{
  "name": "nightingale-string",
  "version": "4.0.4",
  "description": "String handler for nightingale",
  "keywords": [
    "nightingale",
    "nightingale-handler"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/nightingale.git",
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./index.js",
  "typings": "./dist/index.d.ts",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json --declarationMap --emitDeclarationOnly",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "tsc --noEmit && eslint --ext .ts  src/",
    "test": "echo \"No tests\"",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "nightingale-raw-formatter": "^3.0.4",
    "nightingale-types": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-latest-node": "^2.0.0-beta.3",
    "eslint": "^4.19.1",
    "eslint-config-pob": "^20.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-typescript": "^0.12.0",
    "pob-babel": "^22.2.6",
    "prettier": "^1.13.5",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^16.0.0"
  }
}
