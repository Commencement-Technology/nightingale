{
  "name": "nightingale-console-example",
  "private": true,
  "version": "10.0.1",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-console-example"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node10-dev.mjs",
          "require": "./dist/index-node10-dev.cjs.js"
        },
        "import": "./dist/index-node10.mjs",
        "require": "./dist/index-node10.cjs.js"
      }
    },
    "./children": {
      "node": {
        "development": {
          "import": "./dist/children-node10-dev.mjs",
          "require": "./dist/children-node10-dev.cjs.js"
        },
        "import": "./dist/children-node10.mjs",
        "require": "./dist/children-node10.cjs.js"
      }
    }
  },
  "sideEffects": true,
  "scripts": {
    "build": "pob-build",
    "lint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/nightingale-console-example",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "children"
    ]
  },
  "dependencies": {
    "nightingale": "^10.0.1",
    "nightingale-console": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "babel-preset-latest-node": "5.0.0",
    "pob-babel": "25.1.1",
    "rollup": "2.32.0"
  }
}
