{
  "name": "nightingale-example",
  "private": true,
  "version": "10.0.1",
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-example"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./configure": {
      "node": {
        "development": {
          "import": "./dist/configure-node10-dev.mjs",
          "require": "./dist/configure-node10-dev.cjs.js"
        },
        "import": "./dist/configure-node10.mjs",
        "require": "./dist/configure-node10.cjs.js"
      }
    },
    "./console": {
      "node": {
        "development": {
          "import": "./dist/console-node10-dev.mjs",
          "require": "./dist/console-node10-dev.cjs.js"
        },
        "import": "./dist/console-node10.mjs",
        "require": "./dist/console-node10.cjs.js"
      }
    },
    "./debug": {
      "node": {
        "development": {
          "import": "./dist/debug-node10-dev.mjs",
          "require": "./dist/debug-node10-dev.cjs.js"
        },
        "import": "./dist/debug-node10.mjs",
        "require": "./dist/debug-node10.cjs.js"
      }
    },
    "./errors": {
      "node": {
        "development": {
          "import": "./dist/errors-node10-dev.mjs",
          "require": "./dist/errors-node10-dev.cjs.js"
        },
        "import": "./dist/errors-node10.mjs",
        "require": "./dist/errors-node10.cjs.js"
      }
    },
    "./jsonConsole": {
      "node": {
        "development": {
          "import": "./dist/jsonConsole-node10-dev.mjs",
          "require": "./dist/jsonConsole-node10-dev.cjs.js"
        },
        "import": "./dist/jsonConsole-node10.mjs",
        "require": "./dist/jsonConsole-node10.cjs.js"
      }
    }
  },
  "sideEffects": true,
  "scripts": {
    "build": "pob-build",
    "lint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/nightingale-example",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "configure",
      "console",
      "debug",
      "errors",
      "jsonConsole"
    ]
  },
  "dependencies": {
    "nightingale": "^10.0.1",
    "nightingale-console": "^10.0.1",
    "nightingale-console-output": "^10.0.1",
    "nightingale-json-formatter": "^10.0.1",
    "nightingale-string": "^10.0.1",
    "nightingale-types": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "babel-preset-latest-node": "5.0.0",
    "pob-babel": "25.1.1",
    "rollup": "2.32.0"
  }
}
