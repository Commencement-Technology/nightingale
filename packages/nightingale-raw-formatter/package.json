{
  "name": "nightingale-raw-formatter",
  "version": "3.0.6",
  "description": "Nightingale Raw formatter",
  "keywords": [
    "nightingale",
    "nightingale-formatter"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/nightingale.git",
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=8.3.0"
  },
  "browserslist": [
    "> 0.1%",
    "Firefox ESR",
    "not ie < 9",
    "not dead"
  ],
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=lcov --coverageDirectory=docs/coverage/",
    "lint": "npm run typescript-check && eslint --ext .ts  src/",
    "test": "jest",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts",
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "babel-jest"
    }
  },
  "dependencies": {
    "nightingale-formatter": "^4.0.6",
    "nightingale-types": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@types/jest": "23.3.10",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-preset-latest-node": "3.0.1",
    "babel-preset-modern-browsers": "13.0.1",
    "eslint": "5.9.0",
    "eslint-config-pob": "24.0.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-typescript": "0.14.0",
    "jest": "23.6.0",
    "nightingale-levels": "^3.0.4",
    "pob-babel": "23.5.0",
    "prettier": "1.15.3",
    "typescript": "3.2.1",
    "typescript-eslint-parser": "21.0.2",
    "xunit-file": "^1.0.0"
  }
}
