{
  "name": "nightingale-slack",
  "version": "5.0.1",
  "description": "Slack handler for nightingale",
  "keywords": ["nightingale", "nightingale-handler"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/nightingale.git",
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "module:node": "./dist/index-node10.es.js",
  "module:node-dev": "./dist/index-node10-dev.es.js",
  "module:aliases-node": {
    "./createBody.js": "./dist/createBody-node10.es.js"
  },
  "module:aliases-node-dev": {
    "./createBody.js": "./dist/createBody-node10-dev.es.js"
  },
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=lcov --coverageDirectory=docs/coverage/",
    "lint": "npm run typescript-check && eslint --ext .ts  src/",
    "test": "jest",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts",
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverageFrom": ["src/**/*.ts"],
    "moduleFileExtensions": ["ts", "js"],
    "transform": {
      "^.+\\.ts$": "babel-jest"
    }
  },
  "dependencies": {
    "nightingale-levels": "^4.0.0",
    "nightingale-markdown-formatter": "^3.0.1",
    "nightingale-raw-formatter": "^4.0.1",
    "nightingale-types": "^2.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@types/jest": "23.3.10",
    "@types/request": "^2.48.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-preset-latest-node": "3.0.1",
    "babel-register": "^6.24.0",
    "eslint": "5.11.0",
    "eslint-config-pob": "24.0.3",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-typescript": "0.14.0",
    "jest": "23.6.0",
    "pob-babel": "23.5.5",
    "prettier": "1.15.3",
    "typescript": "3.2.2",
    "typescript-eslint-parser": "21.0.2",
    "xunit-file": "^1.0.0"
  }
}
