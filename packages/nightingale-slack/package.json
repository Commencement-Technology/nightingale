{
  "name": "nightingale-slack",
  "version": "10.0.1",
  "description": "Slack handler for nightingale",
  "keywords": [
    "nightingale",
    "nightingale-handler"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-slack"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node10-dev.mjs",
          "require": "./dist/index-node10-dev.cjs.js"
        },
        "import": "./dist/index-node10.mjs",
        "require": "./dist/index-node10.cjs.js"
      }
    },
    "./createBody": {
      "node": {
        "development": {
          "import": "./dist/createBody-node10-dev.mjs",
          "require": "./dist/createBody-node10-dev.cjs.js"
        },
        "import": "./dist/createBody-node10.mjs",
        "require": "./dist/createBody-node10.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=pob-lcov-reporter --coverageDirectory=docs/coverage/",
    "lint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/nightingale-slack",
    "test": "jest",
    "test:watch": "jest",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts",
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "babel-jest"
    },
    "testEnvironment": "node"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index",
      "createBody"
    ]
  },
  "dependencies": {
    "nightingale-levels": "^10.0.1",
    "nightingale-markdown-formatter": "^10.0.1",
    "nightingale-raw-formatter": "^10.0.1",
    "nightingale-types": "^10.0.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@types/jest": "26.0.14",
    "@types/request": "2.48.5",
    "babel-jest": "26.5.2",
    "babel-preset-latest-node": "5.0.0",
    "jest": "26.5.3",
    "pob-babel": "25.1.1",
    "pob-lcov-reporter": "3.0.1",
    "rollup": "2.32.0",
    "xunit-file": "1.0.0"
  }
}
