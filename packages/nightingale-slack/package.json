{
  "name": "nightingale-slack",
  "version": "4.0.6",
  "description": "Slack handler for nightingale",
  "keywords": [
    "nightingale",
    "nightingale-handler"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/nightingale.git",
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./index.js",
  "typings": "./dist/index.d.ts",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "module:aliases-node": {
    "./createBody.js": "./dist/createBody-node8.es.js"
  },
  "module:aliases-node-dev": {
    "./createBody.js": "./dist/createBody-node8-dev.es.js"
  },
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=lcov --coverageDirectory=docs/coverage/",
    "lint": "npm run typescript-check && eslint --ext .ts  src/",
    "test": "jest",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts",
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "babel-jest"
    }
  },
  "dependencies": {
    "nightingale-levels": "^3.0.4",
    "nightingale-markdown-formatter": "^2.0.6",
    "nightingale-raw-formatter": "^3.0.6",
    "nightingale-types": "^1.0.4",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@types/jest": "23.3.1",
    "@types/request": "^2.47.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "babel-preset-latest-node": "2.0.2",
    "babel-register": "^6.24.0",
    "eslint": "5.4.0",
    "eslint-config-pob": "22.0.3",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-typescript": "0.12.0",
    "jest": "23.5.0",
    "pob-babel": "23.2.2",
    "prettier": "1.14.2",
    "typescript": "3.0.3",
    "typescript-eslint-parser": "18.0.0",
    "xunit-file": "^1.0.0"
  }
}
