{
  "name": "nightingale-console",
  "version": "4.0.6",
  "description": "Console handler for nightingale",
  "keywords": [
    "nightingale",
    "nightingale-handler"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/nightingale.git",
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./index.js",
  "typings": "./dist/index.d.ts",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "npm run typescript-check && eslint --ext .ts  src/",
    "test": "echo \"No tests\"",
    "typescript-check": "tsc --noEmit",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "dependencies": {
    "nightingale-ansi-formatter": "^3.0.6",
    "nightingale-console-output": "^3.0.4",
    "nightingale-debug": "^4.0.5",
    "nightingale-types": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0-beta.3",
    "babel-preset-latest-node": "^2.0.0-beta.3",
    "eslint": "^5.4.0",
    "eslint-config-pob": "^22.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-typescript": "^0.12.0",
    "pob-babel": "^22.4.0",
    "prettier": "^1.14.2",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  }
}
