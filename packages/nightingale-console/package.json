{
  "name": "nightingale-console",
  "version": "10.0.1",
  "description": "Console handler for nightingale",
  "keywords": [
    "nightingale",
    "nightingale-handler"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:christophehurpeau/nightingale.git",
    "directory": "packages/nightingale-console"
  },
  "homepage": "https://github.com/christophehurpeau/nightingale",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "development": {
          "import": "./dist/index-node10-dev.mjs",
          "require": "./dist/index-node10-dev.cjs.js"
        },
        "import": "./dist/index-node10.mjs",
        "require": "./dist/index-node10.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pob-build",
    "build:definitions": "tsc -p tsconfig.build.json",
    "clean": "rm -Rf docs dist test/node6 coverage",
    "lint": "yarn --cwd ../.. eslint --ext .js,.ts --report-unused-disable-directives --quiet packages/nightingale-console",
    "watch": "pob-watch"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "10",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "dependencies": {
    "nightingale-ansi-formatter": "^10.0.1",
    "nightingale-console-output": "^10.0.1",
    "nightingale-debug": "^10.0.1",
    "nightingale-json-formatter": "^10.0.1",
    "nightingale-levels": "^10.0.1",
    "nightingale-types": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "babel-preset-latest-node": "5.0.0",
    "pob-babel": "25.1.1",
    "rollup": "2.32.0"
  }
}
